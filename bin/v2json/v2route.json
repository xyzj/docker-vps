{
    "log": {
        "loglevel": "none"
    },
    "routing": {
        "domainMatcher": "mph",
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "domain": [
                    "google-analytics.com",
                    "geosite:category-ads-all"
                ],
                "outboundTag": "block",
                "type": "field"
            },
            {
                "outboundTag": "direct",
                "protocol": [
                    "bittorrent"
                ],
                "type": "field"
            },
            {
                "domain": [
                    "baidu.com",
                    "ecosia.org",
                    "geosite:cn"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "ip": [
                    "192.168.50.0/24",
                    "192.168.1.0/24",
                    "ext:geoip-only-cn-private.dat:cn",
                    "ext:geoip-only-cn-private.dat:private"
                ],
                "outboundTag": "direct",
                "type": "field"
            },
            {
                "domain": [
                    "geosite:gfw"
                ],
                "outboundTag": "proxy",
                "type": "field"
            }
        ]
    }
}