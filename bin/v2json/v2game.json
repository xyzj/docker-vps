{
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 8122,
            "protocol": "http",
            "settings": {
                "accounts": [],
                "allowTransparent": true,
                "timeout": 30,
                "userLevel": 0
            },
            "sniffing": {
                "enabled": false,
                "destOverride": [
                    "http",
                    "tls"
                ],
                "metadataOnly": true
            }
        },
	{
            "listen": "0.0.0.0",
            "port": 8123,
            "protocol": "socks",
            "settings": {
                "accounts": [],
                "auth": "none",
                "ip": "127.0.0.1",
                "udp": true
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
